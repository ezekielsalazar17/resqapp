<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".DashboardUser">


    <Button
        android:id="@+id/back_button"
        android:layout_width="38dp"
        android:layout_height="38dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/border_radius"
        android:drawableStart="@drawable/baseline_arrow_back_24"
        android:padding="7dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck,VisualLintBounds" />


    <TextView
        android:id="@+id/Admin3"
        android:layout_width="110dp"
        android:layout_height="40dp"
        android:layout_marginTop="28dp"
        android:fontFamily="@font/oswald_bold"
        android:letterSpacing="0.03"
        android:paddingStart="23dp"
        android:text="ADMIN"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.435"
        app:layout_constraintStart_toEndOf="@+id/back_button"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="RtlSymmetry,TextSizeCheck" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Admin3"
        app:srcCompat="@drawable/profile_final"
        tools:ignore="ContentDescription" />

    <Button
        android:id="@+id/profile_change"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@drawable/profile_border"
        android:drawableTop="@drawable/baseline_camera_alt_24"
        android:padding="3.5dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView5"
        app:layout_constraintEnd_toEndOf="@+id/imageView5"
        app:layout_constraintHorizontal_bias="0.728"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toTopOf="@+id/imageView5"
        app:layout_constraintVertical_bias="0.829"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/police_name_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/oswald_bold"
        android:text="Name"
        android:textAlignment="center"
        android:textColor="#15224B"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView5"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        tools:ignore="VisualLintOverlap" />

    <TextView
        android:id="@+id/police_contact_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/oswald_light"
        android:text="Contact Number"
        android:textAlignment="center"
        android:textColor="#15224B"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView5"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/police_name_profile" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:background="@drawable/baseline_edit_24"
        app:layout_constraintBottom_toBottomOf="@+id/police_contact_number"
        app:layout_constraintEnd_toEndOf="@+id/imageView5"
        app:layout_constraintHorizontal_bias="0.699"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/police_name_profile"
        app:layout_constraintVertical_bias="0.8"
        tools:ignore="ContentDescription,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/id_num"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginTop="60dp"
        android:fontFamily="@font/oswald_light"
        android:text="@string/id_number_123123"
        android:textAlignment="center"
        android:textColor="#15224B"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView5"
        app:layout_constraintHorizontal_bias="0.095"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/police_contact_number"
        tools:ignore="VisualLintOverlap" />

    <TextView
        android:id="@+id/police_id_num1"
        android:layout_width="wrap_content"
        android:layout_height="25dp"

        android:layout_marginTop="60dp"
        android:fontFamily="@font/oswald_light"
        android:paddingBottom="1dp"
        android:textAlignment="textStart"
        android:textColor="#15224B"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView5"
        app:layout_constraintHorizontal_bias="0.074"
        app:layout_constraintStart_toEndOf="@+id/id_num"
        app:layout_constraintTop_toBottomOf="@+id/police_contact_number" />

    <TextView
        android:id="@+id/id_pres"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/oswald_light"
        android:text="@string/id_presented_philhealth"
        android:textAlignment="center"
        android:textColor="#15224B"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView5"
        app:layout_constraintHorizontal_bias="0.048"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/id_num"
        tools:ignore="VisualLintOverlap" />

    <TextView
        android:id="@+id/ambulance_id_pres1"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/oswald_light"
        android:textAlignment="textStart"
        android:textColor="#15224B"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView5"
        app:layout_constraintHorizontal_bias="0.074"
        app:layout_constraintStart_toEndOf="@+id/id_pres"
        app:layout_constraintTop_toBottomOf="@+id/police_id_num1"
        tools:ignore="VisualLintOverlap" />

    <TextView
        android:id="@+id/email"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/oswald_light"
        android:text="@string/email3"
        android:textAlignment="center"
        android:textColor="#15224B"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView5"
        app:layout_constraintHorizontal_bias="0.12"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/department"
        tools:ignore="VisualLintOverlap" />

    <TextView
        android:id="@+id/police_email1"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/oswald_light"
        android:textAlignment="textStart"
        android:textColor="#15224B"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView5"
        app:layout_constraintHorizontal_bias="0.104"
        app:layout_constraintStart_toEndOf="@+id/email"
        app:layout_constraintTop_toBottomOf="@+id/police_department1"
        tools:ignore="VisualLintOverlap" />

    <TextView
        android:id="@+id/department"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/oswald_light"
        android:text="@string/department"
        android:textAlignment="center"
        android:textColor="#15224B"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView5"
        app:layout_constraintHorizontal_bias="0.059"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/id_pres"
        tools:ignore="VisualLintOverlap" />

    <TextView
        android:id="@+id/police_department1"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/oswald_light"
        android:textAlignment="textStart"
        android:textColor="#15224B"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView5"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintStart_toEndOf="@+id/department"
        app:layout_constraintTop_toBottomOf="@+id/id_pres1"
        tools:ignore="VisualLintOverlap" />

    <Button
        android:id="@+id/sign_out"
        style="?android:attr/buttonBarButtonStyle"
        android:layout_width="145dp"
        android:layout_height="48dp"
        android:layout_marginTop="136dp"
        android:background="@drawable/sign_out"
        android:fontFamily="@font/oswald"
        android:padding="10dp"
        android:text="@string/sign_out"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView5"
        app:layout_constraintHorizontal_bias="0.014"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/email" />

    <Button
        android:id="@+id/about_us"
        style="?android:attr/buttonBarButtonStyle"
        android:layout_width="145dp"
        android:layout_height="48dp"
        android:background="@drawable/sign_out"
        android:fontFamily="@font/oswald"
        android:padding="10dp"
        android:text="@string/about_us"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView5"
        app:layout_constraintStart_toEndOf="@+id/sign_out"
        app:layout_constraintTop_toTopOf="@+id/sign_out"
        tools:ignore="TouchTargetSizeCheck" />


</androidx.constraintlayout.widget.ConstraintLayout>

