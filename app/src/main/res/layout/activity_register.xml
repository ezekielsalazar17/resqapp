<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:ignore="ExtraText">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="vertical"
        >
        <Button
            android:id="@+id/back_button"
            android:layout_width="38dp"
            android:layout_height="38dp"
            android:layout_marginStart="5dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/border_radius"
            android:drawableStart="@drawable/baseline_arrow_back_24"
            android:padding="7dp"
            tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck,VisualLintBounds" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:orientation="horizontal"
        >


        <ImageView
            android:id="@+id/imageView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="41dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="41dp"
            android:layout_marginBottom="16dp"
            app:srcCompat="@drawable/resqapplogo"
            tools:ignore="ContentDescription" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:orientation="vertical"
        >
        <TextView
            android:id="@+id/id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="92dp"
            android:text="@string/id1"
            android:textSize="15sp"
            android:fontFamily="@font/oswald"
            android:textColor="@color/black"
            />
        <EditText
            android:id="@+id/id1"
            android:layout_width="212dp"
            android:layout_height="38dp"
            android:layout_marginStart="100dp"
            android:layout_marginEnd="50dp"
            android:background="@drawable/border"
            android:hint="@string/id"
            android:inputType="text"
            android:letterSpacing="0.09"
            android:paddingStart="10sp"
            android:paddingEnd="10sp"
            android:fontFamily="@font/oswald_light"
            android:textAlignment="textStart"
            android:textSize="12sp"
            tools:ignore="Autofill,MissingConstraints,SmallSp,TextContrastCheck,TouchTargetSizeCheck,DuplicateSpeakableTextCheck"
            />

        <ImageButton
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginStart="280dp"
            android:layout_marginTop="-28dp"
            android:src="@drawable/baseline_folder_24"
            tools:ignore="ContentDescription,SpeakableTextPresentCheck,TouchTargetSizeCheck"
            android:id="@+id/ocrid"/>

    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:orientation="vertical"
        >
        <TextView
            android:id="@+id/fname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="92dp"
            android:fontFamily="@font/oswald"
            android:textSize="15sp"
            android:text="@string/first_name"
            android:textColor="@color/black"
            />
        <EditText
            android:id="@+id/fname1"
            android:layout_width="212dp"
            android:layout_height="38dp"
            android:layout_marginStart="100dp"
            android:layout_marginEnd="100dp"
            android:autofillHints="emailAddress"
            android:background="@drawable/border"
            android:ems="10"
            android:hint="@string/first_name2"
            android:inputType="textEmailAddress"
            android:fontFamily="@font/oswald_light"
            android:letterSpacing="0.09"
            android:paddingStart="10sp"
            android:paddingEnd="10sp"
            android:textAlignment="textStart"
            android:textSize="12sp"
            tools:ignore="DuplicateSpeakableTextCheck,MissingConstraints,TextContrastCheck,TouchTargetSizeCheck"
            tools:layout_editor_absoluteY="572dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:orientation="vertical"
        >
        <TextView
            android:id="@+id/lname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="92dp"
            android:fontFamily="@font/oswald"
            android:textSize="15sp"
            android:text="@string/last_name"
            android:textColor="@color/black"
            />
        <EditText
            android:id="@+id/lname1"
            android:layout_width="212dp"
            android:layout_height="38dp"
            android:layout_marginStart="100dp"
            android:layout_marginEnd="100dp"
            android:autofillHints="emailAddress"
            android:background="@drawable/border"
            android:ems="10"
            android:hint="@string/last_name2"
            android:inputType="textEmailAddress"
            android:fontFamily="@font/oswald_light"
            android:letterSpacing="0.09"
            android:paddingStart="10sp"
            android:paddingEnd="10sp"
            android:textAlignment="textStart"
            android:textSize="12sp"
            tools:ignore="DuplicateSpeakableTextCheck,MissingConstraints,TextContrastCheck,TouchTargetSizeCheck"
            tools:layout_editor_absoluteY="572dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:orientation="vertical"
        >
        <TextView
            android:id="@+id/email1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="92dp"
            android:fontFamily="@font/oswald"
            android:textSize="15sp"
            android:text="Email"
            android:textColor="@color/black"
            />
        <EditText
            android:id="@+id/useremail1"
            android:layout_width="212dp"
            android:layout_height="38dp"
            android:layout_marginStart="100dp"
            android:layout_marginEnd="100dp"
            android:autofillHints="emailAddress"
            android:background="@drawable/border"
            android:ems="10"
            android:hint="@string/email2"
            android:inputType="textEmailAddress"
            android:fontFamily="@font/oswald_light"
            android:letterSpacing="0.09"
            android:paddingStart="10sp"
            android:paddingEnd="10sp"
            android:textAlignment="textStart"
            android:textSize="12sp"
            tools:ignore="DuplicateSpeakableTextCheck,MissingConstraints,TextContrastCheck,TouchTargetSizeCheck"
            tools:layout_editor_absoluteY="572dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:orientation="vertical"
        >
        <TextView
            android:id="@+id/contactuser1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="92dp"
            android:fontFamily="@font/oswald"
            android:textSize="15sp"
            android:text="@string/contact1"
            android:textColor="@color/black"
            />
        <EditText
            android:id="@+id/contactuser2"
            android:layout_width="212dp"
            android:layout_height="38dp"
            android:layout_marginStart="100dp"
            android:layout_marginEnd="100dp"
            android:autofillHints="emailAddress"
            android:background="@drawable/border"
            android:ems="10"
            android:hint="@string/contact2"
            android:inputType="textEmailAddress"
            android:fontFamily="@font/oswald_light"
            android:letterSpacing="0.09"
            android:paddingStart="10sp"
            android:paddingEnd="10sp"
            android:textAlignment="textStart"
            android:textSize="12sp"
            tools:ignore="DuplicateSpeakableTextCheck,MissingConstraints,TextContrastCheck,TouchTargetSizeCheck"
            tools:layout_editor_absoluteY="572dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:orientation="vertical"
        >
        <TextView
            android:id="@+id/userpass"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="92dp"
            android:text="@string/password1"
            android:fontFamily="@font/oswald"
            android:textColor="@color/black"
            android:textSize="15sp"
            />
        <EditText
            android:id="@+id/userpass1"
            android:layout_width="212dp"
            android:layout_height="38dp"
            android:layout_marginStart="100dp"
            android:layout_marginEnd="100dp"
            android:background="@drawable/border"
            android:ems="10"
            android:hint="@string/password"
            android:inputType="textPassword"
            android:letterSpacing="0.09"
            android:paddingStart="10sp"
            android:paddingEnd="10sp"
            android:textAlignment="textStart"
            android:fontFamily="@font/oswald_light"
            android:textSize="12sp"
            tools:ignore="Autofill,MissingConstraints,TextContrastCheck,TouchTargetSizeCheck,DuplicateSpeakableTextCheck"
            tools:layout_editor_absoluteY="449dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:orientation="vertical"
        >
        <TextView
            android:id="@+id/userconpass"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="92dp"
            android:text="@string/confirm_password1"
            android:fontFamily="@font/oswald"
            android:textColor="@color/black"
            android:textSize="15sp"
            />

        <EditText
            android:id="@+id/userconpass1"
            android:layout_width="212dp"
            android:layout_height="38dp"
            android:layout_marginStart="100dp"
            android:layout_marginEnd="101dp"
            android:background="@drawable/border"
            android:ems="10"
            android:fontFamily="@font/oswald_light"
            android:hint="@string/confirmpassword"
            android:inputType="textPassword"
            android:letterSpacing="0.09"
            android:paddingStart="10sp"
            android:paddingEnd="10sp"
            android:textAlignment="textStart"
            android:textSize="12sp"
            tools:ignore="Autofill,MissingConstraints,TouchTargetSizeCheck,DuplicateSpeakableTextCheck,TextContrastCheck"
            />
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="74dp"
        android:orientation="vertical">

        <Button
            android:id="@+id/register"
            android:layout_width="212dp"
            android:layout_height="38dp"
            android:layout_marginStart="100dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="172dp"
            android:background="@drawable/red_border"
            android:hint="@string/register"
            android:letterSpacing="0.4"
            android:textColorHint="@color/white"
            android:textStyle="bold"
            tools:ignore="TouchTargetSizeCheck" />

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:orientation="vertical"
        >

        <ProgressBar
            android:id="@+id/progressBar"
            style="?android:attr/progressBarStyle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="invisible" />

    </LinearLayout>

</LinearLayout>