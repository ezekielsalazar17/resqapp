<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/dashboard_user"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >

    <View
        android:layout_width="match_parent"
        android:layout_height="3px"
        android:background="#A9A9A9"
        android:layout_margin="10dp"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="65dp"
        tools:ignore="MissingConstraints"

        />

    <ImageButton
        android:id="@+id/profile_button"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/profile_button"
        android:padding="0dp"
        android:scaleType="fitCenter"
        android:src="@drawable/profile_final"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="ContentDescription,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="298dp"
        android:layout_height="168dp"
        android:layout_marginTop="64dp"
        android:padding="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/resqapplogo"
        tools:ignore="ContentDescription" />

    <ImageButton
        android:id="@+id/coastguard_button"
        android:layout_width="160dp"
        android:layout_height="147dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/button_dashboard_user"
        android:padding="20dp"
        android:scaleType="fitXY"
        android:src="@drawable/boat"
        app:layout_constraintEnd_toEndOf="@+id/police_button"
        app:layout_constraintHorizontal_bias="0.583"
        app:layout_constraintStart_toStartOf="@+id/police_button"
        app:layout_constraintTop_toBottomOf="@+id/police_text"
        tools:ignore="ContentDescription,SpeakableTextPresentCheck,ImageContrastCheck,VisualLintBounds" />

    <ImageButton
        android:id="@+id/fire_button"
        android:layout_width="160dp"
        android:layout_height="147dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/button_dashboard_user"
        android:padding="20dp"
        android:scaleType="fitXY"
        android:src="@drawable/firefighter"
        app:layout_constraintEnd_toEndOf="@+id/location_tracking"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/location_tracking"
        app:layout_constraintTop_toBottomOf="@+id/location_tracking"
        tools:ignore="ContentDescription,SpeakableTextPresentCheck,ImageContrastCheck,VisualLintBounds" />

    <ImageButton
        android:id="@+id/police_button"
        android:layout_width="160dp"
        android:layout_height="147dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/button_dashboard_user"
        android:padding="20dp"
        android:scaleType="fitXY"
        android:src="@drawable/police"
        app:layout_constraintEnd_toEndOf="@+id/location_tracking"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/location_tracking"
        app:layout_constraintTop_toBottomOf="@+id/location_tracking"
        tools:ignore="ContentDescription,SpeakableTextPresentCheck,ImageContrastCheck,VisualLintBounds" />

    <TextView
        android:id="@+id/ambulance_text"
        android:layout_width="130sp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/oswald"
        android:letterSpacing="0.05"
        android:text="@string/ambulance_department"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/ambulance_button"
        app:layout_constraintStart_toStartOf="@+id/ambulance_button"
        app:layout_constraintTop_toBottomOf="@+id/ambulance_button" />

    <TextView
        android:id="@+id/police_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="17dp"
        android:fontFamily="@font/oswald"
        android:letterSpacing="0.05"
        android:text="@string/police_department"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/police_button"
        app:layout_constraintStart_toStartOf="@+id/police_button"
        app:layout_constraintTop_toBottomOf="@+id/police_button" />

    <TextView
        android:id="@+id/fire_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/oswald"
        android:letterSpacing="0.05"
        android:text="@string/fire_department"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/fire_button"
        app:layout_constraintHorizontal_bias="0.467"
        app:layout_constraintStart_toStartOf="@+id/fire_button"
        app:layout_constraintTop_toBottomOf="@+id/fire_button" />

    <TextView
        android:id="@+id/coast_text"
        android:layout_width="130sp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/oswald"
        android:letterSpacing="0.05"
        android:text="@string/coast_guard_department"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/coastguard_button"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="@+id/coastguard_button"
        app:layout_constraintTop_toBottomOf="@+id/coastguard_button" />

    <ImageButton
        android:id="@+id/ambulance_button"
        android:layout_width="160dp"
        android:layout_height="147dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/button_dashboard_user"
        android:padding="20dp"
        android:scaleType="fitXY"
        android:src="@drawable/ambulance"
        app:layout_constraintEnd_toEndOf="@+id/fire_button"
        app:layout_constraintStart_toStartOf="@+id/fire_button"
        app:layout_constraintTop_toBottomOf="@+id/fire_text"
        tools:ignore="ContentDescription,SpeakableTextPresentCheck,ImageContrastCheck,VisualLintBounds" />

    <Button
        android:id="@+id/location_tracking"
        android:layout_width="330dp"
        android:layout_height="40dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/location_button"
        android:drawableStart="@drawable/baseline_location_on_24"
        android:fontFamily="@font/oswald"
        android:hint="@string/location_tracking"
        android:paddingStart="100dp"
        android:textAlignment="textStart"
        android:textColorHint="@color/white"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        tools:ignore="MissingConstraints,RtlSymmetry,TextContrastCheck,TouchTargetSizeCheck,VisualLintButtonSize" />

</androidx.constraintlayout.widget.ConstraintLayout>
