<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".DashboardUser">


    <Button
        android:id="@+id/back_button"
        android:layout_width="38dp"
        android:layout_height="38dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/border_radius"
        android:drawableStart="@drawable/baseline_arrow_back_24"
        android:padding="7dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck,VisualLintBounds" />


    <TextView
        android:id="@+id/USER"
        android:layout_width="110dp"
        android:layout_height="40dp"
        android:layout_marginTop="28dp"
        android:fontFamily="@font/oswald_bold"
        android:letterSpacing="0.03"
        android:paddingStart="23dp"
        android:text="USER"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.435"
        app:layout_constraintStart_toEndOf="@+id/back_button"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="RtlSymmetry,TextSizeCheck" />

    <ImageView
        android:id="@+id/previewimage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/USER"
        android:background="@drawable/white_circle"
        app:srcCompat="@drawable/profile_final"
        tools:ignore="ContentDescription" />


    <TextView
        android:id="@+id/user_name_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/oswald_bold"
        android:text="Name"
        android:textAlignment="center"
        android:textColor="#15224B"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="@+id/previewimage"
        app:layout_constraintStart_toStartOf="@+id/previewimage"
        app:layout_constraintTop_toBottomOf="@+id/previewimage"
        tools:ignore="VisualLintOverlap" />

    <TextView
        android:id="@+id/user_contact_number"
        android:layout_width="320dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/oswald_light"
        android:text="Contact Number"
        android:textAlignment="center"
        android:textColor="#15224B"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/previewimage"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="@+id/previewimage"
        app:layout_constraintTop_toBottomOf="@+id/user_name_profile" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:background="@drawable/baseline_edit_24"
        app:layout_constraintBottom_toBottomOf="@+id/user_contact_number"
        app:layout_constraintEnd_toEndOf="@+id/user_contact_number"
        app:layout_constraintHorizontal_bias="0.7"
        app:layout_constraintStart_toStartOf="@+id/user_contact_number"
        app:layout_constraintTop_toBottomOf="@+id/user_name_profile"
        app:layout_constraintVertical_bias="0.8"
        tools:ignore="ContentDescription,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/id_num"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginTop="64dp"
        android:fontFamily="@font/oswald_light"
        android:text="@string/id_number_123123"
        android:textAlignment="center"
        android:textColor="#15224B"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/user_contact_number"
        app:layout_constraintHorizontal_bias="0.055"
        app:layout_constraintStart_toStartOf="@+id/user_contact_number"
        app:layout_constraintTop_toBottomOf="@+id/user_contact_number"
        tools:ignore="VisualLintOverlap" />

    <TextView
        android:id="@+id/useridnum"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginTop="64dp"
        android:fontFamily="@font/oswald_light"
        android:paddingBottom="1dp"
        android:textAlignment="textStart"
        android:textColor="#15224B"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/previewimage"
        app:layout_constraintHorizontal_bias="0.073"
        app:layout_constraintStart_toEndOf="@+id/id_num"
        app:layout_constraintTop_toBottomOf="@+id/user_contact_number" />

    <TextView
        android:id="@+id/id_pres"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/oswald_light"
        android:text="@string/id_presented_philhealth"
        android:textAlignment="center"
        android:textColor="#15224B"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/user_contact_number"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/user_contact_number"
        app:layout_constraintTop_toBottomOf="@+id/id_num"
        tools:ignore="VisualLintOverlap" />

    <TextView
        android:id="@+id/idshow"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/oswald_light"
        android:textAlignment="textStart"
        android:textColor="#15224B"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/previewimage"
        app:layout_constraintHorizontal_bias="0.073"
        app:layout_constraintStart_toEndOf="@+id/id_pres"
        app:layout_constraintTop_toBottomOf="@+id/useridnum"
        tools:ignore="VisualLintOverlap" />

    <TextView
        android:id="@+id/birthday"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/oswald_light"
        android:text="@string/birthday"
        android:textAlignment="center"
        android:textColor="#15224B"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/user_contact_number"
        app:layout_constraintHorizontal_bias="0.092"
        app:layout_constraintStart_toStartOf="@+id/user_contact_number"
        app:layout_constraintTop_toBottomOf="@+id/usernum"
        tools:ignore="VisualLintOverlap" />

    <TextView
        android:id="@+id/user_email"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/oswald_light"
        android:text="@string/user_email"
        android:textAlignment="center"
        android:textColor="#15224B"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/user_contact_number"
        app:layout_constraintHorizontal_bias="0.157"
        app:layout_constraintStart_toStartOf="@+id/user_contact_number"
        app:layout_constraintTop_toBottomOf="@+id/birthday"
        tools:ignore="VisualLintOverlap" />

    <TextView
        android:id="@+id/user_email1"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/oswald_light"
        android:textAlignment="textStart"
        android:textColor="#15224B"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/previewimage"
        app:layout_constraintHorizontal_bias="0.056"
        app:layout_constraintStart_toEndOf="@+id/user_email"
        app:layout_constraintTop_toBottomOf="@+id/user_birthday1"
        tools:ignore="VisualLintOverlap" />

    <TextView
        android:id="@+id/user_birthday1"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/oswald_light"
        android:textAlignment="textStart"
        android:textColor="#15224B"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/previewimage"
        app:layout_constraintHorizontal_bias="0.017"
        app:layout_constraintStart_toEndOf="@+id/birthday"
        app:layout_constraintTop_toBottomOf="@+id/user_number1"
        tools:ignore="VisualLintOverlap" />

    <TextView
        android:id="@+id/usernum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/oswald_light"
        android:text="@string/user_number"
        android:textAlignment="center"
        android:textColor="#15224B"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/user_contact_number"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/user_contact_number"
        app:layout_constraintTop_toBottomOf="@+id/id_pres"
        tools:ignore="VisualLintOverlap" />

    <TextView
        android:id="@+id/user_number1"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/oswald_light"
        android:textAlignment="textStart"
        android:textColor="#15224B"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/previewimage"
        app:layout_constraintHorizontal_bias="0.062"
        app:layout_constraintStart_toEndOf="@+id/usernum"
        app:layout_constraintTop_toBottomOf="@+id/idshow"
        tools:ignore="VisualLintOverlap" />

    <Button
        android:id="@+id/sign_out"
        style="?android:attr/buttonBarButtonStyle"
        android:layout_width="145dp"
        android:layout_height="48dp"
        android:layout_marginTop="144dp"
        android:background="@drawable/sign_out"
        android:fontFamily="@font/oswald"
        android:padding="10dp"
        android:text="@string/sign_out"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/user_contact_number"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="@+id/user_contact_number"
        app:layout_constraintTop_toBottomOf="@+id/birthday"
        app:layout_constraintVertical_bias="0.0" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/selectimg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/black"
        android:clickable="true"
        app:fabSize="mini"
        app:layout_constraintBottom_toTopOf="@+id/user_name_profile"
        app:layout_constraintEnd_toEndOf="@+id/previewimage"
        app:layout_constraintHorizontal_bias="0.937"
        app:layout_constraintStart_toStartOf="@+id/previewimage"
        app:layout_constraintTop_toBottomOf="@+id/USER"
        app:layout_constraintVertical_bias="0.875"
        app:srcCompat="@drawable/baseline_camera_alt_24"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />


</androidx.constraintlayout.widget.ConstraintLayout>

